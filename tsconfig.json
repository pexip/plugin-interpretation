{
  "compilerOptions": {
    "jsx": "react",
    "target": "ESNext",                                  
    "module": "ESNext",                               
    "moduleResolution": "bundler",                       
    "esModuleInterop": true,                          
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,         
    "strict": true,                                   
    "skipLibCheck": true                              
  }
}